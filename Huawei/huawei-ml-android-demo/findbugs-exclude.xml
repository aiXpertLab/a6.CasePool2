<?xml version="1.0"?>
<FindBugsFilter>

    <Match>
        <Class
            name="com.huawei.mlkit.example.language.textembedding.TextEmbeddingAnalyseActivity" />
    </Match>
    <Match>
        <Class name="com.huawei.mlkit.example.body.face3d.Still3DFaceAnalyseActivity" />
        <Or>
            <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON" />
            <Bug pattern="SBSC_USE_STRINGBUFFER_CONCATENATION" />
        </Or>
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.body.handgesture.StillHandGestureAnalyseActivity" />
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.body.handkeypoint.StillHandKeyPointAnalyseActivity" />
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.custommodel.CustomModelActivity" />
        <Or>
            <Bug pattern="SE_NO_SERIALVERSIONID" />
            <Bug pattern="OS_OPEN_STREAM_EXCEPTION_PATH" />
            <Bug pattern="SE_COMPARATOR_SHOULD_BE_SERIALIZABLE" />
        </Or>
    </Match>

    <Match>
        <Class
            name="com.huawei.mlkit.example.image.classification.ImageClassificationAnalyseActivity" />
    </Match>
    <Match>
        <Class name="com.huawei.mlkit.example.image.landmark.ImageLandmarkAnalyseActivity" />
    </Match>
    <Match>
        <Class
            name="com.huawei.mlkit.example.image.scenedetection.SenceDetectionStillAnalyseActivity" />
    </Match>


    <Match>
        <Class name="com.huawei.mlkit.example.text.document.ImageDocumentAnalyseActivity" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.text.generalCard.PassCardProcess" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.text.text.ImageTextAnalyseActivity" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.voice.sounddect.SoundDectActivity" />
        <Or>
            <Bug pattern="SBSC_USE_STRINGBUFFER_CONCATENATION" />
        </Or>

    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.voice.translate.TranslatorActivity" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.voice.tts.TtsOnlineModeActivity" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.voice.tts.TtsOfflineModeActivity" />
    </Match>

    <Match>
        <Class
            name="com.huawei.mlkit.example.body.livenessdetection.LiveLivenessDetectionActivity" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.image.productvisionsearch.ProductFragment" />
    </Match>

    <Match>
        <Class name="com.huawei.mlkit.example.voice.aft.AftAnalyseActivity" />
        <Or>
            <Bug pattern="LI_LAZY_INIT_STATIC" />
        </Or>
    </Match>

</FindBugsFilter>
